    <section id="chat">
        <section id="result">
            <p>Jij antwoordde answer1</p>
            <p>Maaike antwoordde answer1</p>
        </section>
        <section class="chat_msg" id="msg1"><p>Hey! hoe gaat het?</p></section>
        <section class="chat_msg" id="msg2"><p>Goed! en met jou?</p></section>
    </section>
    <section id="popUpWrapper">
        <section id="popUp">
            <a href="#"><i class="fa fa-chevron-left"></i>
                <p>Terug naar matches</p>
            </a>
            <section id="questions">
                <p>Kies een vraag om samen met je match te beantwoorden:</p>
                {{#each randVraag }}
                <section class="quest" id="vr_id{{this.id}}">
                    <p>{{ this.vraag }}</p>
                </section>
                {{/each}}

            </section>
                {{#each randVraag }}
            <section class="answers invisible">
                <h3>{{ this.vraag }}</h3>
                <form method="POST" action="/" id="ant_id{{this.id}}">
                    <fieldset>
                        <label><input type="radio" name="answer" id="ans1" value="{{this.ant1}}">&nbsp;{{this.ant1}}</label>
                    </fieldset>
                    <fieldset>
                        <label><input type="radio" name="answer" id="ans2" value="{{this.ant2}}">&nbsp;{{this.ant2}}</label>
                    </fieldset>
                    <input type="submit" value="Confirm answer" id="sendAnswer">
                </form>
            </section>
                {{/each}}
        </section>
    </section>